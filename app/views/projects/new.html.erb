<h1>New project</h1>

<% form_for(@project) do |f| %>
  <%= f.error_messages %>
  <%= f.hidden_field :created_by, { :value => session[:user_id] } %>
  
  <%= render :partial => "project", :object => @project, :locals => { :f => f } %>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', projects_path %>